<h1>Users</h1>
<table class="table">
<thead>
    <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Email</th>
    </tr>
</thead>
<tbody>
    {{#each users}}
        <tr>
            <td>{{ this.name }}</td>
            <td>{{ this.email }}</td>
        </tr>
    {{/each}}
</tbody>
</table>